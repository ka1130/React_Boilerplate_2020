    "test": "jest",
    "eslint": "eslint 'src/**/*.js?(x)' --report-unused-disable-directives",
    "eslint-fix": "eslint --fix 'src/**/*.js?(x)' || true",
    "build": "webpack --mode production",
    "serve": "http-server dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
