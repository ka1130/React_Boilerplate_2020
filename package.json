    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "react-testing-library": "^8.0.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.1",
